	<string>We need access to documents directory to store albums created by user</string>
	<key>UIFileSharingEnabled</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.176483108320-nahh7ecaibk3dr3tb38r3jkq4c3l9cip</string>
			</array>
		</dict>
	</array>
